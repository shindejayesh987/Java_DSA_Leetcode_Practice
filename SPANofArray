class Main {
  public static void main(String[] args) {
    int[] arr = {52,6,8,10,98,65};
    int max = arr[0];
    int min = arr[0];

    for(int i = 0; i < arr.length ; i++){
      if(arr[i] > max){
        max = arr[i];
      }
      if(arr[i] < min ){
        min = arr[i];
      }
    }
    int span = max -min ;

    System.out.print(span);
    
  }
}
